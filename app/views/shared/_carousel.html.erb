
<div id="home" class="carousel slide" data-ride="carousel" style="padding-top: 50px; margin-top: -10px;">
  <div class="carousel-indicators">
    <li data-target="#carouselExampleControls" data-slide-to="0"> 
    <li data-target="#carouselExampleControls" data-slide-to="1">
    <li data-target="#carouselExampleControls" data-slide-to="2">
    <li data-target="#carouselExampleControls" data-slide-to="3">
    <li data-target="#carouselExampleControls" data-slide-to="4">
  </div>
  <div class="carousel-inner">
    <div class="carousel-item active">
      <%= image_tag "carousel/CNeryTours-Home-Carousel-01.jpg", alt: "First slide",  class: "d-block w-100"%>   
    </div>
    <div class="carousel-item">
      <%= image_tag "carousel/CNeryTours-Home-Carousel-02.jpg", alt: "Second slide",  class: "d-block w-100"%>
    </div>
    <div class="carousel-item">
      <%= image_tag "carousel/CNeryTours-Home-Carousel-03.jpg", alt: "Third slide",  class: "d-block w-100"%>
    </div>
    <div class="carousel-item">
      <%= image_tag "carousel/CNeryTours-Home-Carousel-04.jpg", alt: "Third slide",  class: "d-block w-100"%>
    </div>
    <div class="carousel-item">
      <%= image_tag "carousel/CNeryTours-Home-Carousel-05.jpg", alt: "Third slide",  class: "d-block w-100"%>
    </div>
  </div>
  <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
